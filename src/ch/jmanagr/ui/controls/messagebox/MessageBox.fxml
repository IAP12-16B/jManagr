<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<AnchorPane fx:id="messageBoxPane" focusTraversable="true" prefHeight="300.0" prefWidth="450.0"
            xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="ch.jmanagr.ui.controls.messagebox.MessageBoxController">
    <children>
        <Label fx:id="messageLabel" alignment="TOP_LEFT" cache="true" layoutX="14.0" layoutY="14.0" prefHeight="179.0"
               prefWidth="422.0" text="Label" wrapText="true" AnchorPane.bottomAnchor="70.0"
               AnchorPane.leftAnchor="14.0" AnchorPane.rightAnchor="14.0" AnchorPane.topAnchor="51.0"/>
        <Button fx:id="okButton" layoutX="329.0" layoutY="261.0" mnemonicParsing="false" onAction="#clickOk"
                prefHeight="25.0" prefWidth="107.0" text="OK" AnchorPane.bottomAnchor="14.0"
                AnchorPane.rightAnchor="14.0"/>
        <Label fx:id="messageBoxTitle" alignment="BASELINE_CENTER" layoutX="14.0" layoutY="14.0" prefHeight="25.0"
               prefWidth="422.0" styleClass="h1" text="Label" textAlignment="CENTER" AnchorPane.leftAnchor="14.0"
               AnchorPane.rightAnchor="14.0" AnchorPane.topAnchor="14.0">
            <font>
                <Font name="System Bold" size="16.0"/>
            </font>
        </Label>
    </children>
</AnchorPane>
