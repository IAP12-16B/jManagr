<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane id="Content" fx:id="departmentView" minHeight="0.0" minWidth="0.0" prefHeight="588.0" prefWidth="769.0"
            xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="ch.jmanagr.ui.departments.DepartmentController">
    <children>
        <TableView fx:id="depTable" editable="true" layoutX="14.0" layoutY="14.0" prefHeight="250.0" prefWidth="740.0"
                   AnchorPane.bottomAnchor="324.0" AnchorPane.leftAnchor="14.0" AnchorPane.rightAnchor="14.0"
                   AnchorPane.topAnchor="14.0">
            <columns>
                <TableColumn fx:id="idCol" editable="false" prefWidth="75.0" text="ID"/>
                <TableColumn fx:id="nameCol" editable="true" prefWidth="75.0" text="Name"/>
            </columns>
        </TableView>
        <TextField fx:id="nameField" layoutX="14.0" layoutY="272.0" prefHeight="25.0" prefWidth="75.0"
                   AnchorPane.bottomAnchor="290.0" AnchorPane.leftAnchor="14.0"/>
        <Button fx:id="addBtn" layoutX="101.0" layoutY="271.0" mnemonicParsing="false" onAction="#addDep"
                text="Hinzufügen" AnchorPane.bottomAnchor="291.0" AnchorPane.leftAnchor="101.0"/>
        <Button fx:id="delBtn" layoutX="197.0" layoutY="271.0" mnemonicParsing="false" onAction="#deleteDep"
                text="Löschen" AnchorPane.bottomAnchor="291.0" AnchorPane.leftAnchor="197.0"/>
    </children>
</AnchorPane>
