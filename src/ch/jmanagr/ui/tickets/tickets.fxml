<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane id="Content" fx:id="ticketView" cache="false" minHeight="0.0" minWidth="0.0" prefHeight="588.0"
            prefWidth="769.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="ch.jmanagr.ui.tickets.TicketController">
    <children>
        <TableView fx:id="ticketTable" editable="true" layoutX="14.0" layoutY="14.0" prefHeight="200.0"
                   prefWidth="741.0">
            <columns>
                <TableColumn fx:id="idCol" editable="false" prefWidth="75.0" text="ID"/>
                <TableColumn fx:id="departmentCol" editable="false" prefWidth="75.0" text="Department"/>
                <TableColumn fx:id="nameCol" editable="false" prefWidth="75.0" text="Name"/>
                <TableColumn fx:id="dateCol" editable="false" prefWidth="75.0" text="Datum"/>
                <TableColumn fx:id="userCol" editable="false" prefWidth="75.0" text="Benutzer"/>
                <TableColumn fx:id="agentCol" editable="false" prefWidth="75.0" text="Agent"/>
                <TableColumn fx:id="resourceCol" editable="false" prefWidth="75.0" text="Ressource"/>
            </columns>
        </TableView>
        <Button fx:id="newTicketBtn" layoutX="14.1875" layoutY="223.5" mnemonicParsing="false" onAction="#newTicket"
                text="Neues Ticket"/>
    </children>
</AnchorPane>
